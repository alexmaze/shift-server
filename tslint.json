{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": [false],
    "no-reference": false,
    "ordered-imports": [false],
    "member-ordering": [false],
    "no-empty": false,
    "no-require-imports": false,
    "member-access": [false],
    "no-var-requires": false,
    "prefer-for-of": false
  }
}
