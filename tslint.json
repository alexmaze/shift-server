{
  "extends": "tslint:recommended",
  "rules": {
    "semicolon": [
      true, "never"
    ],
    "quotemark": [
      true, "double"
    ],
    "trailing-comma": [true, {
      "multiline": "never",
      "singleline": "never"
    }],
    "object-literal-sort-keys": false,
    "no-console": [
      false
    ],
    "no-reference": false,
    "ordered-imports": [
      false
    ],
    "member-access": [
      false
    ],
    "member-ordering": [
      false
    ],
    "max-line-length": [
      false
    ],
    "no-empty": false,
    "no-require-imports": false,
    "no-var-requires": false
  },
  "jsRules": {
  },
  "rulesDirectory": [
  ]
}
